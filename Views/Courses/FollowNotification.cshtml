@model BigSchool.ViewModels.FollowNotificationViewModel
@{
    ViewBag.Title = "Notification Page";
}
<h2>FollowingNotification</h2>
<div class="row">
    <ul class="course">
        @foreach (var follow in Model.Notifications)
        {
            <li>
                <span class="lecturer">
                    @follow.Logger
                    <br />
                </span>
            </li>
        }
    </ul>
</div>
